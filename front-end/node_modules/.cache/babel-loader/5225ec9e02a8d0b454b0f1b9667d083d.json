  \"isSerious\" : this.isSerious,\n                    \"actInf\" : this.actInf\n                };//发送的数据\n\n                this.$request.post(\"/pubAct\",postData,{\n                    headers : {\n                        'Content-Type':'application/json;charset=utf-8' //跨域字段处处理，json格式好\n                    }\n                }).then((result) => {\n                    console.log(result);\n                }).catch((err) => {\n                    console.log(err);\n                })\n\n            },\n            catchTime(time){\n                console.log(time);//time是一个数组\n                this.actTime = time;\n            },\n            catchDate(date){\n                console.log(date);\n                this.actDate = date;\n            }\n\n        }\n    }\n</script>\n\n<style scoped>\n\n#pubAction{\n    border: 1px solid black;\n    width: 70%;\n}\n\n#form{\n    display: flex;\n    flex-direction: column;\n}\n\n.form-box{\n    display: flex;\n    flex-direction: row;\n    border: 1px solid red;\n    margin : 10px auto;\n}\n\n</style>"],"sourceRoot":"src/components/indexPage"}]}