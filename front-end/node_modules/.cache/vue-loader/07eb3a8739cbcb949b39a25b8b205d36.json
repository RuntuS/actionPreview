"Block\">\n            <Col offset=\"0\">\n                <Button v-on:click=\"Login()\" type=\"primary\">登陆</Button>\n            </Col>\n        </Row>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: \"LoginBox\",\n        data() {\n            return {\n                account : \"\",\n                password : \"\"\n            }\n        },\n        methods:{\n            Login(){\n                console.log(this.account + this.password);\n                this.$request.get(\"/login\",{\n                    params : {\n                        aco : this.account ,\n                        psw : this.password\n                    }\n                })\n                .then(res => {\n                    console.log(res.data);\n                    let objOfName = res.data;\n                    if (objOfName.length !== 0)\n                    {\n                        console.log(objOfName[0].name);\n                        alert(objOfName[0].name + \", 您已经登陆成功!\");\n                        this.$router.push('/index');\n                    }\n                    else{\n                        alert(\"账号密码错误，请重试!\");\n                    }\n                }).catch(\n                    (err) => {\n                        console.log(err);\n                    }\n                )\n            }\n        },\n        beforeMount() {\n\n        }\n    }\n</script>\n\n<style scoped>\n#FirstBox{\n    border: 1px solid black;\n    width: 40%;\n    position: relative;\n}\n.Block{\n    margin-top: 5%;\n}\n\n.Block:last-child{\n    margin-bottom: 2%;\n}\n\n\n\n</style>"]}]}